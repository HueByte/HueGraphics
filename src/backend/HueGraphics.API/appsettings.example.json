{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "PointCloudSettings": {
    "DataPath": "ABSOLUTE_PATH_TO_POINTCLOUD_DATA_FOLDER"
  },
  "UploadSettings": {
    "ApiKey": "YOUR_SECRET_API_KEY_HERE",
    "MaxFileSizeMB": 100,
    "AllowedExtensions": [".zip"],
    "UploadPath": "uploads"
  },
  "ModelParserSettings": {
    "ExecutablePath": "model_parser",
    "DefaultPointCount": 50000
  }
}
