# API Configuration
# Copy this file to .env.development for development or .env.production for production
#
# For production (Docker), use relative path:
# VITE_API_BASE_URL=/api
#
# For development, use full URL:
# VITE_API_BASE_URL=http://localhost:5000/api

VITE_API_BASE_URL=/api

# Feature Flags
# Set to true to enable Kinect live capture feature
VITE_KINECT_ENABLED=false
